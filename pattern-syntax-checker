import java.util.Scanner;
import java.util.regex.Pattern;
import java.util.regex.PatternSyntaxException;

public class Solution {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        // Read the number of test cases
        int testCases = scanner.nextInt();
        scanner.nextLine(); // Consume the newline character
        
        for (int i = 0; i < testCases; i++) {
            String pattern = scanner.nextLine(); // Read the regex pattern
            
            // Check if the pattern is valid
            try {
                Pattern.compile(pattern); // Try to compile the pattern
                System.out.println("Valid"); // If successful, print Valid
            } catch (PatternSyntaxException e) {
                System.out.println("Invalid"); // If an exception is thrown, print Invalid
            }
        }
        
        scanner.close(); // Close the scanner
    }
}




